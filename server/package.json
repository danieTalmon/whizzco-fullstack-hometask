{
  "name": "node-ts-seed",
  "version": "0.0.0",
  "description": "Simple seed for your NodeJS projects made with typescript.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ludohenin/node-ts-seed"
  },
  "scripts": {
    "start": "ts-node --files src/index.ts",
    "build": "rimraf dist && npm run lint && tsc -p ./tsconfig.json",
    "lint": "tslint --fix **/src/**/*.ts",
    "lint:fix": "tslint --fix **/src/**/*.ts"
  },
  "keywords": [
    "typescript",
    "tsc",
    "node",
    "nodejs",
    "seed",
    "starter",
    "vscode",
    "debugging"
  ],
  "author": "ludohenin <ludovic.henin@yahoo.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.9",
    "chai": "^3.5.0",
    "eslint": "^8.3.0",
    "mocha": "^9.1.3",
    "rimraf": "^2.5.2",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7",
    "typings": "^1.0.4"
  },
  "dependencies": {
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "stream-json": "^1.7.3",
    "uuid": "^8.3.0"
  }
}
